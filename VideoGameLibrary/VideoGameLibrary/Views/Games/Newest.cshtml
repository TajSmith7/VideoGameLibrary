@model List<VideoGameLibrary.Models.Game>

<h2>Newest Games</h2>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Release Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var game in Model)
        {
            <tr>
                <td>@game.Id</td>
                <td>@game.Name</td>
                <td>@game.ReleaseDate?.ToString("yyyy-MM-dd")</td>
            </tr>
        }
    </tbody>
</table>
